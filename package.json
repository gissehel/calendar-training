{
  "name": "express-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Express + React + Redux + Weppack 4 + babel 7 + hot reload server + hot reload client",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node ./src/server.js",
    "debug": "NODE_ENV=development babel-node --inspect ./src/server.js",
    "server": "NODE_ENV=production babel-node ./src/server.js",
    "build": "NODE_ENV=production ./node_modules/.bin/babel-node ./node_modules/.bin/webpack --mode production",
    "build-dev": "NODE_ENV=development ./node_modules/.bin/babel-node ./node_modules/.bin/webpack --mode development",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook"
  },
  "author": "gissehel",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "history": "^4.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-centered": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.0.2",
    "css-loader": "^3.2.0",
    "css-modules-require-hook": "^4.2.3",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "htmllint-loader": "^2.1.4",
    "mini-css-extract-plugin": "^0.8.0",
    "react-hot-loader": "^4.12.12",
    "semantic-ui-react": "^0.88.0",
